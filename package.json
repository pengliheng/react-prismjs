{
  "name": "react-prismjs",
  "version": "1.0.3",
  "description": "use react with prism.js",
  "main": "./dist/index.js",
  "scripts": {
    "eslint": "eslint ./src --color --fix",
    "b": "cross-env NODE_ENV=development PLATFORM=web webpack --color",
    "p": "cross-env NODE_ENV=production PLATFORM=web webpack -p --color",
    "t": "webpack-dev-server --open --color",
    "dev": "node build/dev-server.js",
    "build": "node build/build.js",
    "test": "cross-env NODE_ENV=test && mocha test/*",
    "lint": "./node_modules/.bin/eslint ./ --ext js,jsx --ignore-pattern .eslintignore --cache --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pengliheng/react-prismjs"
  },
  "author": "pengliheng",
  "keywords": [
    "node",
    "prismjs",
    "highlight",
    "react"
  ],
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.7",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "prismjs": "^1.8.3",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1"
  }
}
